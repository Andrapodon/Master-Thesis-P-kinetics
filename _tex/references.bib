@article{mariniNewFertilizerRegulation2020,
  title = {The New Fertilizer Regulation: {{A}} Starting Point for Cadmium Control in {{European}} Arable Soils?},
  shorttitle = {The New Fertilizer Regulation},
  author = {Marini, Michele and Caro, Dario and Thomsen, Marianne},
