@article{sreeramaNutrientsAntinutrientsCowpea2012,
  title = {Nutrients and Antinutrients in Cowpea and Horse Gram Flours in Comparison to Chickpea Flour: {{Evaluation}} of Their Flour Functionality},
  shorttitle = {Nutrients and Antinutrients in Cowpea and Horse Gram Flours in Comparison to Chickpea Flour},
