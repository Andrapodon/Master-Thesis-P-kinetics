	langid = {english},
	file = {McElderry et al. - 2013 - Crystallinity and compositional changes in carbona.pdf:/home/marc/Zotero/storage/3T66JUCH/McElderry et al. - 2013 - Crystallinity and compositional changes in carbona.pdf:application/pdf},
