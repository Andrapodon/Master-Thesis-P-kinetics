@article{arcieriRiceCultivationItaly2020,
  title = {Rice Cultivation in {{Italy}} under the Threat of Climatic Change: {{Trends}}, Technologies and Research Gaps},
  shorttitle = {Rice Cultivation in {{Italy}} under the Threat of Climatic Change},
  author = {Arcieri, Marco and Ghinassi, Graziano},
